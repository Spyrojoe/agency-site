				} else if (scrollPos > 6919 && scrollPos < 8709){
					$('a[href="#visionworks"]').css('color', '#fff');
					$('a[href="#einstein_noah"]').css('color', '#fff');
					$('a[href="#universal"]').css('color', '#fff');
					$('a[href="#curves"]').css('color', '#fff');
					$('a[href="#bmw"]').css('color', '#fff');
					$('a[href="#stage_stores"]').css('color', '#fff');
					$('a[href="#wingstop"]').css('color', '#33ccff');
					$('a[href="#yoshinoya"]').css('color', '#fff');
				} else {
					$('a[href="#visionworks"]').css('color', '#fff');
					$('a[href="#einstein_noah"]').css('color', '#fff');
					$('a[href="#universal"]').css('color', '#fff');
					$('a[href="#curves"]').css('color', '#fff');
					$('a[href="#bmw"]').css('color', '#fff');
					$('a[href="#stage_stores"]').css('color', '#fff');
					$('a[href="#wingstop"]').css('color', '#fff');
					$('a[href="#yoshinoya"]').css('color', '#33ccff');
				}
			});

			
			
			

			function toggleSubnav(){
				if($(window).width() > 640){
					$('#work_side_sub').show();
				}
			}
		});
	</script>
	
    </body>
</html>
