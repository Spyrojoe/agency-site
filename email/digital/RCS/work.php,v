	<?php if($toCollapse != 'collapseOne'): ?>
		<script>
			var scrollToAnchor;
			var id_obj = {};
			var isMobile = false;
			var isSafari = false;
			var active_id = '#wingstop_nav';
			var _offset = 0;
			var wW = $(window).width();
			$(function(){
				// location in /js/main.js
				$('.accordion-heading').first().addClass('active');
				$('.accordion-toggle').each(function(){
						if($(this).attr('href') == "#<?php echo $toCollapse;?>"){
							$(this).trigger('click');
						}
				});
				
				//Is Mobile?
				if(wW<767) isMobile = true;
				//Animates to the anchor tags
				scrollToAnchor = function(aid)
				{
					var client_tag = $(aid);
					var ele_id = aid.replace('#','');
					if(!isMobile)
					{
						$('html,body').animate({scrollTop: client_tag.offset().top + 3},'fast');
						return false;
					} else {
						if($('.show-nav').length > 0)
						{
							showNav();
						}
						return true;	
					}
				}
				//Listener for the scrolling; detects where in the window is each DIV id
				if(!isMobile)
				{
					$(window).scroll(function(e) {
						makeActive();
					});
				} else {
					var inactive_area=false;
					var active_area = false;
					var wingstop_top = $('#wingstop').offset().top;
					$(window).scroll(function(){
						_offset = $(window).scrollTop();
						if(wingstop_top < _offset)
						{
							//Scrolling Past Wingstop
							if(!active_area)
							{
								//This Code only happens once
								active_area=true;
								inactive_area=false;
								$('#mobile_toTop').css('display','block');
							}
						} else {
							//Scrolling Before Wingstop
							if(!inactive_area)
							{
								//This code only happens once
								active_area=false;
								inactive_area=true;
								$('#mobile_toTop').css('display','none');
							}
						}
						//makeActive($('#interactive_content'));
						//makeActive();
					});
				}
				//Function below Activates and Deactivates the 'active' class in the work subnav
				function makeActive()
				{
					var w_top = $(window).scrollTop() + 3;
					var wingstop_top = $('#wingstop').offset().top;
					var curves_top = $('#curves').offset().top;
					var noah_top = $('#einstein_noah').offset().top;
					var visionworks_top = $('#visionworks').offset().top;
					var stage_top = $('#stage_stores').offset().top;
					var yoshinoya_top = $('#yoshinoya').offset().top;
					var universal_top = $('#universal').offset().top;
					var bmw_top = $('#bmw').offset().top;
										
					if(curves_top > w_top)
					{
						active_id = '.wingstop_nav';
					} else if((w_top > curves_top) && (noah_top > w_top))
					{
						active_id = '.curves_nav';
					} else if((w_top > noah_top) && (visionworks_top > w_top))
					{
						active_id = '.noah_nav';
					} else if((w_top > visionworks_top) && (stage_top > w_top))
					{
						active_id = '.visionworks_nav';
					} else if((w_top > stage_top) && (yoshinoya_top > w_top))
					{
						active_id = '.stage_nav';
					} else if((w_top > yoshinoya_top) && (universal_top > w_top))
					{
						active_id = '.yoshinoya_nav';
					} else if((w_top > universal_top) && (bmw_top > w_top))
					{
						active_id = '.universal_nav';
					} else if(w_top > bmw_top)
					{
						active_id = '.bmw_nav';
					}
					resetActives(active_id);
					function resetActives(id)
					{
						var nav_array = Array('.wingstop_nav','.curves_nav','.noah_nav','.visionworks_nav','.stage_nav','.yoshinoya_nav','.universal_nav','.bmw_nav');
						for(var i in nav_array)
						{
							if(id != nav_array[i]) $(nav_array[i]).removeClass('active');
							else $(nav_array[i]).addClass('active');
						}
					}
				}
			});
			$(window).load(function() {
				$('.img').each(function(index, element) {
					var img_blk = $(this).find('.img_blk');
					var img_obj = img_blk.find('img')
					var img_w = img_obj.width();
					var ratio = 1;
					//_offset = $('#side_nav').height() + $('.active_sub_nav').height();
					var orig_w = 650;//maximum size possible for image area.
					var new_w = 970;//Maximum size of container
					new_w = new_w - ((new_w/100) * 33);
					ratio = new_w/orig_w;
					var img_container_w = $(this).outerWidth(true);
					
					var percent = ((img_w/img_container_w)*100);
					percent = percent * ratio;
					img_blk.css('width',percent + '%');
					img_obj.css({
						width:'100%',
						height:'auto'
					});
				});
				$('.work_client').each(function(index, element) {
					var _id = $(this).attr('id');
					var _pos = $(this).offset().top - $('#interactive_content').position().top;
					id_obj[_id] = _pos;
				});
				if(isMobile) $('.wingstop_nav').addClass('active');
			});
		</script>
	<?php endif; ?>
